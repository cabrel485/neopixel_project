perform_userdel() {
	local rootdir="$1"
	local opts="$2"
	bbnote "petalinux-user-image: Performing userdel with [$opts]"
	local username=`echo "$opts" | awk '{ print $NF }'`
	local user_exists="`grep "^$username:" $rootdir/etc/passwd || true`"
	if test "x$user_exists" != "x"; then
