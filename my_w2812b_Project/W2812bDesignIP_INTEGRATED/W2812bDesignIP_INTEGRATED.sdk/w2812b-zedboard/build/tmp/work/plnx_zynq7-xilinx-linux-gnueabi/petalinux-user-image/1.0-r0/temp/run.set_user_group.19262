perform_groupdel() {
	local rootdir="$1"
	local opts="$2"
	bbnote "petalinux-user-image: Performing groupdel with [$opts]"
	local groupname=`echo "$opts" | awk '{ print $NF }'`
	local group_exists="`grep "^$groupname:" $rootdir/etc/group || true`"

	if test "x$group_exists" != "x"; then
		local awk_input='BEGIN {FS=":"}; $1=="'$groupname'" { print $3 }'
		local groupid=`echo "$awk_input" | awk -f- $rootdir/etc/group`
		local awk_check_users='BEGIN {FS=":"}; $4=="'$groupid'" {print $1}'
		local other_users=`echo "$awk_check_users" | awk -f- $rootdir/etc/passwd`

		if test "x$other_users" = "x"; then
			eval flock -x $rootdir/etc -c \"$PSEUDO groupdel \$opts\" || true
			group_exists="`grep "^$groupname:" $rootdir/etc/group || true`"
			if test "x$group_exists" != "x"; then
				bbfatal "petalinux-user-image: groupdel command did not succeed."
			fi
		else
			bbnote "petalinux-user-image: '$groupname' is primary group for users '$other_users', not removing it"
		fi
	else
		bbnote "petalinux-user-image: group $groupname doesn't exist, not removing it"
	fi
}

